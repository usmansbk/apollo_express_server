{
  "name": "graphql-server",
  "version": "1.0.0",
  "description": "Apollo Graphql server",
  "main": "index.js",
  "scripts": {
    "start": "node -r dotenv/config build/bin/www dotenv_config_path=./.env.production",
    "dev": "nodemon -r dotenv/config build/bin/www dotenv_config_path=./.env.development",
    "build": "npx babel src -d build --copy-files",
    "build:watch": "npx babel src -d build --copy-files --watch",
    "dbcreate": "npx sequelize db:create",
    "test": "jest",
    "doc": "npx graphdoc --force"
  },
  "repository": {
    "type": "git",
    "url": "git+//https://github.com/usmansbk/apollo_express_server.git"
  },
  "graphdoc": {
    "endpoint": "http://localhost:4000/graphql",
    "output": "./docs"
  },
  "keywords": [
    "boilerplate",
    "apollo",
    "server",
    "expressjs"
  ],
  "author": "Usman Suleiman",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/usmansbk/apollo_express_server/issues"
  },
  "homepage": "https://github.com/usmansbk/apollo_express_server#readme",
  "dependencies": {
    "@babel/runtime": "^7.10.5",
    "apollo-server-express": "^2.15.1",
    "bunyan": "^1.8.14",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql-tools": "^6.0.13",
    "pg": "^8.3.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.3.3"
  },
  "devDependencies": {
    "@2fd/graphdoc": "^2.4.0",
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-optional-chaining": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "jest": "^26.1.0",
    "nodemon": "^2.0.4",
    "sequelize-cli": "^6.2.0"
  }
}
